---
export const prerender = false;
import { actions } from "astro:actions";
import CardsComp from "../components/Cards.svelte";
import Layout from "../layouts/Layout.astro";
import { metadata } from "../store/atproto";
const cards = await Astro.callAction(actions.getCards, {}).then((u) => u.data);
---

<Layout>
    <CardsComp cardsData={cards} meta={metadata} client:only="svelte" />
</Layout>
